{
    "name": "dotgix/chatsupport",
    "description": "This Laravel library package will include live chat support system along with admin and consultant side in the project.",
    "type": "library",
    "require": {
        "pusher/pusher-php-server": "^7.2"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Dotgix\\Chatsupport\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Dotgix Technologies"
        }
    ],
    "minimum-stability": "stable",  // Change minimum-stability to stable
    "prefer-stable": true,  // Ensure Composer prefers stable versions
    "version": "1.0.0",  // Add a version number for the release
    "extra": {
        "laravel": {
            "providers": [
                "Dotgix\\Chatsupport\\ChatSupportServiceProvider"
            ]
        }
    }
}
